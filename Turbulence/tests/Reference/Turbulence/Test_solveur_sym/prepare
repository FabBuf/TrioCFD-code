#!/bin/bash
copie_cas_test cpu_3D_turbulence || exit -1
mv cpu_3D_turbulence/cpu_3D_turbulence.data .
sed "s/precond/save_matrice precond/" cpu_3D_turbulence.data > mod.data 

if [ "$TRUST_DISABLE_MPI" != 1 ]
then
   trust mod || exit -1
   make_PAR.data mod
   trust PAR_mod 2
else
   trust mod || exit -1
fi
