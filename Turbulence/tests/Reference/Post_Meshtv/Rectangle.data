# Hydraulique 2D laminaire : schema upwind #
# PARALLEL OK #
dimension 2

Pb_hydraulique pb
Domaine dom
Domaine dom_post

# BEGIN MESH #
Mailler dom
{
	Pave Entree 
	{
	Origine 0. 0.
	Nombre_de_Noeuds 47 23
	Longueurs 0.92 0.22
	}
	{
	Bord Entree X = 0. 0. <= Y <= 0.22
	Bord Haut Y = 0.22 0. <= X <= 0.92
	Bord Bas Y = 0. 0. <= X <= 0.92
	Bord Sortie X = 0.92 0. <= Y <= 0.22
	}
}

Mailler dom_post
{
	Pave Entree 
	{
	Origine 0. 0.
	Nombre_de_Noeuds 12 7
	Longueurs 0.92 0.22
	}
	{
	Bord Entree X = 0. 0. <= Y <= 0.22
	Bord Haut Y = 0.22 0. <= X <= 0.92
	Bord Bas Y = 0. 0. <= X <= 0.92
	Bord Sortie X = 0.92 0. <= Y <= 0.22
	}
}

# END MESH #

# BEGIN PARTITION
Partition dom
{
	Partition_tool tranche { tranches 2 1 }
	Larg_joint 1
	zones_name DOM
}
Partition dom_post
{
	Partition_tool tranche { tranches 2 1 }
	Larg_joint 1
	zones_name DOMP
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOMP.Zones dom_post
Scatter DOM.Zones dom
END SCATTER #

VDF dis

Scheme_euler_explicit sch
Read sch
{
	tinit 0
	nb_pas_dt_max 20
	tmax 0.25
	dt_min 0.001
	dt_max 0.001
	dt_impr 0.001
	dt_sauv 100
	seuil_statio 1.e-8
}

Fluide_Incompressible eau
Read eau
{
	mu Champ_Uniforme 1 0.02
	rho Champ_Uniforme 1 2
}

Associate pb dom
Associate pb sch
Associate pb eau

Discretize pb dis

Read pb
{
	Navier_Stokes_standard
	{
		solveur_pression GCP { precond ssor { omega 1.5 } seuil 1e-11 impr }	
		convection { amont }
		diffusion { }
		initial_conditions {
			vitesse Champ_Uniforme 2 0. 0.
			}
		boundary_conditions {
			Haut paroi_fixe
			Bas paroi_fixe
			Sortie frontiere_ouverte_pression_imposee Champ_Front_Uniforme 1 0.
			Entree frontiere_ouverte_vitesse_imposee Champ_Front_Uniforme 2  1. 0.
		}
	}

	Post_processings
	{
		first_post
		{
			Probes
			{
				sonde_pression pression periode 0.001 points 2 0.13 0.105 0.13 0.115
				sonde_vitesse vitesse periode 0.001 points 2 0.12 0.105 0.12 0.115
			}
			format lata
			fields dt_post 0.001
			{
				pression som
				pression elem
				vitesse elem
			}
                	Statistiques_en_serie dt_integr 0.1
                	{
				t_deb 0. t_fin 0.25
                   		Moyenne vitesse elem
                   		Moyenne pression elem
                	}
			
			
		}
		second_post
		{
			fichier Post_Meshtv
			fields dt_post 0.1
			{
				pression elem
				vitesse elem
			}
		}
	}
}

Solve pb
End

