# Thermohydraulique 2D : fields par morceaux #
# PARALLEL OK #
dimension 2
Pb_Thermohydraulique pb
Domaine dom
# BEGIN MESH #
Mailler dom
{
	Pave Cavite 
		{
		Origine 0. 0.
     		Nombre_de_Noeuds 11 11
		Longueurs 0.01 0.01
		}
		{
		Bord Gauche X = 0.  0. <= Y <= 0.01
		Bord Haut Y = 0.01  0. <= X <= 0.01
		Bord Bas Y = 0.     0. <= X <= 0.01
		Bord Droit X = 0.01 0. <= Y <= 0.01
		}
}

Sous_Zone zone1
Associate zone1 dom
Read zone1 {
   Rectangle
      Origine 0. 0.
      Cotes 0.005 0.01 }
Sous_Zone zone2
Associate zone2 dom
Read zone2 {
   Rectangle
      Origine 0.005 0.
      Cotes 0.005 0.01 }
Sous_Zone zone3
Associate zone3 dom
Read zone3 {
   Rectangle
      Origine 0.007 0.002
      Cotes   0.001 0.001 }
Sous_Zone zone4
Associate zone4 dom
Read zone4 {
   Rectangle
      Origine 0.002 0.007
      Cotes   0.001 0.001 }
# END MESH #

# BEGIN PARTITION
Partition dom
{
	Tranche 2 1
	Nb_parts 2
	Larg_joint 2
	zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
Sous_Zone zone1
Associate zone1 dom
Read zone1 { Fichier zone1.ssz }
Sous_Zone zone2
Associate zone2 dom
Read zone2 { Fichier zone2.ssz }
Sous_Zone zone3
Associate zone3 dom
Read zone3 { Fichier zone3.ssz }
Sous_Zone zone4
Associate zone4 dom
Read zone4 { Fichier zone4.ssz }
END SCATTER #

VDF dis

Scheme_euler_explicit sch
Read sch
{
	nb_pas_dt_max 2
	tinit 0.
	tmax 1.2
	dt_min 0.001
	dt_max 0.001
	dt_impr 0.001
	dt_sauv 100
	seuil_statio 1.e-8
}

Fluide_Incompressible fluide
Read fluide
{
	mu Champ_Uniforme 1 1.85e-5
	rho Champ_Uniforme 1 1.17
	lambda Champ_Uniforme 1 0.0262 
	Cp Champ_Uniforme 1 1006
	beta_th Champ_Uniforme 1 3.41e-3
}

Champ_Uniforme gravite
Read gravite 2 0 -9.81

Associate fluide gravite
Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis

Read pb
{
	Navier_Stokes_standard
	{
		solveur_pression GCP { 
			precond ssor { omega 1.5 } 
			seuil 1.e-10
			impr 
		}
		convection { Quick }
		diffusion { }
		sources { boussinesq_temperature { T0 5. } }
		initial_conditions {
			vitesse Champ_Uniforme_Morceaux dom 2
			{
				defaut 0. 0.
				zone1 0. -0.01
				zone2 0.  0.01
			}
		}
		boundary_conditions {
			Haut paroi_fixe
			Bas paroi_fixe
			Gauche paroi_fixe
			Droit paroi_fixe
		}
	}
	Convection_Diffusion_Temperature
	{
		diffusion { }
		convection { quick }
		sources {
			Puissance_thermique Champ_Uniforme_Morceaux dom 1
				{ defaut 0. zone3 100000. }
		}
		boundary_conditions 
		{
			Haut paroi_adiabatique
			Bas paroi_adiabatique
 			Gauche paroi_echange_externe_impose T_ext Champ_Front_Uniforme 1 0.  H_imp Champ_Front_Uniforme 1  1.e11
			Droit  paroi_echange_externe_impose T_ext Champ_Front_Uniforme 1 0.  H_imp Champ_Front_Uniforme 1  1.e11
		}
		initial_conditions {
			Temperature Champ_Uniforme_Morceaux dom 1
				{ defaut 0. zone4 10. }
		}
	}

	Post_processing
	{
		Probes
		{
		   sonde_V vitesse periode 0.01 segment 10 0.0005 0.005 0.0095 0.005
			sonde_pression pression periode 0.01 points 1 0.0045 0.0045
			sonde_vitesse vitesse periode 0.01 points 2 0.0025 0.005 0.0075 0.005
			sonde_temp temperature periode 0.01 points 2 0.0075 0.0025 0.0025 0.0075
		}
		format lml
		fields dt_post 0.7
		{
			pression elem
			pression som
			vitesse elem
			vitesse som
			temperature elem
			temperature som
		}
	}
}

Solve pb
End

