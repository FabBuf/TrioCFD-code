# Hydraulique 2D VEF : Paroi defilante #
# PARALLEL OK 8 #
dimension 2
Pb_hydraulique_turbulent pb
Domaine dom
# BEGIN MESH #
Read_file dom keps_2D_VEF_Def.geom
VerifierCoin dom { }
# END MESH #

# BEGIN PARTITION
Partition dom
{
	Partition_tool metis { Nb_parts 2 }
	Larg_joint 2
	zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1B dis

Scheme_euler_explicit sch
Read sch
{
	nb_pas_dt_max 2
	tinit 0.
        tmax 2.
	dt_min 0.0001
	dt_max 0.1
	dt_impr 0.0001
        diffusion_implicite 1
	seuil_diffusion_implicite 1.e-5
	# impr_diffusion_implicite 1 #
	facsec 0.2
	dt_sauv 20
        seuil_statio 1.e-8
}

Fluide_Incompressible fluide
Read fluide
{
	mu Champ_Uniforme 1 2e-5
	rho Champ_Uniforme 1 1.
}

Champ_Uniforme gravite
Read gravite 2 0 -9.81

Associate fluide gravite
Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis

Read pb
{
	Navier_Stokes_turbulent
	{
		solveur_pression petsc cholesky { }
		convection { muscl }
		diffusion { }
		initial_conditions {
		        vitesse Champ_Uniforme 2 0. 0.
			}
                boundary_conditions {
			HAUT Paroi_defilante Champ_Front_Uniforme 2 1. 0.
			BAS paroi_fixe
			EST paroi_fixe
			OUEST paroi_fixe
                }
                modele_turbulence K_Epsilon {
                        Transport_K_Epsilon 
                        {
                                convection { muscl }
                                diffusion { }
                                boundary_conditions {
			        HAUT paroi
			        BAS paroi
			        EST paroi
			        OUEST paroi
                         }
                         initial_conditions {
                                        k_Eps Champ_Uniforme 2 1. 1.
                                }
                         }
                         turbulence_paroi loi_standard_hydr
                } 
	}

	Post_processing
	{
		Probes
		{
			sonde_pression pression periode 0.001 points 1 0.5 0.5
			sonde_vitesse   vitesse periode 0.001 points 1 0.5 0.5
			sonde_U vitesse periode 0.001 segment 51 0.5 0.0 0.5 1.0
			sonde_V vitesse periode 0.001 segment 51 0.0 0.5 1.0 0.5
			sonde_P pression periode 1. plan 30 30 0. 0. 1. 0. 0. 1.
			sonde_KV k   periode 0.001 segment 51 0.5 0.0 0.5 1.0
			sonde_EV eps periode 0.001 segment 51 0.5 0.0 0.5 1.0
			sonde_KH k   periode 0.001 segment 51 0.0 0.5 1.0 0.5
			sonde_EH eps periode 0.001 segment 51 0.0 0.5 1.0 0.5
		}
		fields dt_post 2.
		{
			pression elem
			vitesse som
			viscosite_turbulente som
			k som
			eps som
		}
	}
}

Solve pb
End
