# lance_test 1 ecart #
dimension 3
Pb_Hydraulique_Turbulent pb

Domaine DOM_BODY

Read_unsupported_ASCII_file_from_ICEM DOM_BODY trio_BODY_geo_2prismes.asc

# Tailles 0.8 2.0 0.8 #

Corriger_frontiere_periodique { Domaine DOM_BODY Bord PERIO } 

Transformer DOM_BODY x y z 
#
ecrire_fic_meshtv DOM_BODY DOM_BODY.meshtv
#
VEFPreP1b dis

Schema_Euler_implicite sch
Read sch 
{
        tinit 0.
	nb_pas_dt_max 3
        tmax 30.
        dt_min 1.e-7
        dt_max 1.
	dt_start dt_calc
        dt_impr 0.01
        dt_sauv 10.
        seuil_statio 1.e-12
        facsec 5
	facsec_max 500
        Solveur
           Implicite
           {
                seuil_convergence_solveur 1.e-8
           }
}




Associate pb DOM_BODY
Associate pb sch

Discretize pb dis

Read pb
	{
   
   fluide_incompressible {
    	mu Champ_Uniforme 1 0.0001
   	rho Champ_Uniforme 1 1
   }
   
   	Navier_Stokes_Turbulent
		{
		solveur_pression GCP { precond ssor { omega 1.5 } seuil 1e-8 }
                convection { EF_stab { } }
                diffusion  {  }
                Sources { Canal_perio { bord PERIO } }
		conditions_initiales
			{
                        vitesse Champ_fonc_xyz DOM_BODY 3  10. 0.  0.
			}
		boundary_conditions
			{
			PERIO   periodique 
			HAUT    paroi_fixe 
			BAS     paroi_fixe
			FRONT   Symetrie
			BACK    Symetrie
			}
		modele_turbulence K_Epsilon {
		    Transport_K_Epsilon
		    {
		         convection { EF_stab { } }
		         diffusion { }
		         boundary_conditions {
				PERIO   periodique 
			 	HAUT    paroi 
				BAS     paroi
				FRONT   Symetrie
				BACK    Symetrie
		         }
			 parametre_equation parametre_implicite 
			{
				resolution_explicite
			}
		         conditions_initiales { k_eps Champ_Uniforme 2 1 0.5 }
		    }
		     turbulence_paroi loi_expert_hydr {  methode_calcul_face_keps_impose que_les_faces_des_elts_dirichlet } dt_impr_ustar 4
		    }
		Traitement_particulier { Canal
			{
       			dt_impr_moy_spat 4.
       			}
					}
	}

	Postraitement
        	{       
                Sondes
                	{ 
                        sonde_vit  vitesse periode 10. segment 50 0.05 0. 0.05 0.05 2. 0.05
			sonde_visc_turb  viscosite_turbulente periode 10. segment 50	0.05 0. 0.05  0.05 2. 0.05
			sonde_k K periode 10. segment 50	0.05 0. 0.05  0.05 2. 0.05
			sonde_eps eps periode 10. segment 50	0.05 0. 0.05  0.05 2. 0.05
			}
		format lml
		Champs dt_post 10
			{
			vitesse elem
			viscosite_turbulente elem
			pression elem
			y_plus elem
			}
		}
	}
EcritureLectureSpecial 0
Solve pb
End
 
