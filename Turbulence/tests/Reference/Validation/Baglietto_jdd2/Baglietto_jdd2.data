# PARALLEL OK #
dimension 3
Pb_hydraulique_Turbulent pb
Domaine dom

# BEGIN MESH #
Mailler dom
{
	Pave Duct 
		{
		Origine 0. 0. 0.
		Nombre_de_Noeuds 12 12 3
		Longueurs 0.5 0.5 0.14
		facteurs 1.1 0.9 1
		}
		{
		Bord gauche    X = 0.  0. <= Y <= 0.5 0. <= Z <= 0.14
		Bord droite    X = 0.5 0. <= Y <= 0.5 0. <= Z <= 0.14
		Bord bas       Y = 0.  0. <= X <= 0.5 0. <= Z <= 0.14
		Bord haut      Y = 0.5 0. <= X <= 0.5 0. <= Z <= 0.14
		Bord devant_derriere  Z = 0.  0. <= X <= 0.5 0. <= Y <= 0.5
		Bord devant_derriere  Z = 0.14 0. <= X <= 0.5 0. <= Y <= 0.5
		}
	
} 

Tetraedriser_homogene_compact dom

Corriger_frontiere_periodique { Domaine dom Bord devant_derriere }
# END MESH #

# BEGIN PARTITION 
Partition dom
{
	Partitionneur metis { Nb_parts 2 }
	Larg_joint 2
	Nom_Zones DOM
	Periodique 1 devant_derriere
}
End
 END PARTITION #

# BEGIN SCATTER  
Scatter DOM.Zones dom
 END SCATTER #

VEFPreP1B dis


Schema_Euler_implicite sch
Read sch
{
	tinit 0.
	nb_pas_dt_max 3
	tmax  30.
        # dt_start dt_calc #	
	dt_min 1.e-8
	dt_max 1.
	dt_impr 1.
	dt_sauv 10.
	seuil_statio 1.e-8
	facsec 1.
	facsec_max 20
	Solveur Implicite
	{
		solveur gmres { diag seuil 1.e-13 impr controle_residu 1 nb_it_max 5 }
	}
}

Fluide_incompressible fluide

Read fluide
{	mu Champ_Uniforme  1 1.98e-5
	rho Champ_uniforme 1 1.17
}
 
Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis

Read pb
{
	Navier_Stokes_turbulent
	{
		# solveur_pression GCP { 
			    precond ssor { omega 1.5 } 
			    seuil 1.e-6 
			    impr } #
		solveur_pression  petsc cholesky { }
		Sources { Canal_perio { bord devant_derriere } }	    
		convection { ef_stab { }  }
		diffusion { }
		conditions_initiales {
			vitesse Champ_Uniforme 3 0. 0. 0.690563077
					}
		boundary_conditions {
			gauche symetrie
			droite paroi_fixe
			bas paroi_fixe
			haut symetrie
			devant_derriere periodique
			}

      		 modele_turbulence K_Epsilon
                {
           	   	Transport_K_Epsilon
                  		{
			# with_nu yes #
           		  convection { ef_stab { } }
                 	  diffusion { }
                 	  boundary_conditions
                	    {
				gauche symetrie
				droite paroi
				bas paroi
				haut symetrie
				devant_derriere periodique                            
          	 	    }
           	 	  conditions_initiales 
           	 	   {
              			k_Eps Champ_Uniforme 2 1. 0.7
                           }
            			}
			# Modele_Fonc_Bas_Reynolds standard_KEps { Reynolds_stress_isotrope 0 } #
         	turbulence_paroi loi_standard_hydr
       	       }
	}
	Postraitement
	{
	        
		Format lml
                Sondes
                {
			sonde_k 			K periode 0.1 segment 20 0.5 0.0 0.07 0.5 1 0.07    

		}
		Champs dt_post 30.
		   {
			pression elem
			vitesse
			k
			eps
			viscosite_turbulente
			y_plus
		   }
	}

# reprise xyz duct_pb.xyz #
          
}



Solve pb
End

