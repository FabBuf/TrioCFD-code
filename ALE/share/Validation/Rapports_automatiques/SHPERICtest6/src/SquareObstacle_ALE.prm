Parameters {
        Title "TrioCFD ALE FOD validation"
        Author "R. P."
        TestCase .  SquareObstacle_ALE.data
        VersionTRUST "1.7.5"
	ParametersTRUST Problem : Pb_hydraulique
	ParametersTRUST Dimension : 2D
	ParametersTRUST Domain : Domaine_ALE
	ParametersTRUST Discretization : VEFPre1B
	ParametersTRUST Time scheme : Schema_euler_explicite
	ParametersTRUST Medium : Fluide_Incompressible
	ParametersTRUST Hydraulic problem : Navier_Stokes_standard
        Description "This automated validation test case was created to compare TrioCFD ALE first order discretization with the SPHERIC benchmark 6 [1]. First order discretization means in this case that there is no ALE mesh velocity term in the mass conservation equation and the the Jacobian is unity in the momentum conservation equation. The geometry is taken from [1] and the test case with Reynolds number equal to 50 was chosen."
	IncludeData 1
        Reference "1. A. Colagrossi, 'SPHERIC benchmark 6: 2D Incompressible flow around a moving square inside a rectangular box. - 2D SPH Validation.', SPHERIC, [Online]. Available: http://spheric-sph.org/tests/test-6. [Accessed Sept. 29, 2017]."
}

Chapter {
	Title "Plots and figures"

	Figure {
		Title "Drag coefficient pressure component"
		Description "The drag coefficient pressure component \latex_( $C_{d}(\text{pressure component})=\Delta p /(0.5 \cdot \rho \cdot U_{max}^{2})$ \latex_), where \latex_( $\rho=1 \frac{kg}{m^{3}}, U_{max}=1 \frac{m}{s} (\text{maximum velocity of the obstacle})$ \latex_), is plotted for the following cases: SPHERIC benchmark 6, TrioCFD 1.7.5 and for the new TrioCDF version."
		LabelX "t"
		LabelY "Cd (pressure component)"
		legend below title "Legende"
		Curve {
			legend "SPHERIC"
			origin "[1]"
			file SPHERICtest6.csv
			columns $1 $2
			style dots
		}
		Curve {
			
			legend "TrioCFD 1.7.5"
			file TRIOCFDdata.csv
			columns $1 $2
			style dots
		}
		Curve {
			legend "new TrioCFD"
			file SquareObstacle_ALE_SONDE_DRAG_COEF_PRESSURE_COMP.son
			columns ($1) ($2)
			style dots
		}
	}
	Visu {
		title "VITESSE SOM"
		vector SquareObstacle_ALE.lata dom VITESSE SOM
	}
	visu {
		title "VITESSE_X SOM"
		pseudocolor SquareObstacle_ALE.lata dom VITESSE_X SOM
	}

	visu {
		title "VITESSE_Y SOM"
		pseudocolor SquareObstacle_ALE.lata dom VITESSE_Y SOM
	}
	Visu {
		title "PRESSION SOM"
		pseudocolor SquareObstacle_ALE.lata dom PRESSION SOM
	}
}

