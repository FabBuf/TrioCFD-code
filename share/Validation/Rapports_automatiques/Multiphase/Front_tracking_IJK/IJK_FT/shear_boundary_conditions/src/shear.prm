		titre "Shear periodicity avec defilement : Etat final (PRESSURE)"
		Normal3d 0 1 0
		Up3D 1 0 0
		zoom3D 0 0 1.
		pseudocolor perio_shear_defilant.lata DOM PRESSURE ELEM
	}
}


