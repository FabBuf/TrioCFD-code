# SIMULATION D UN CANAL PLAN 2D avec modele Longueur de melange #
# PARALLEL RUNS #
# Tests du modele Longueur de Melange et de la procedure de calcul du frottement parietal (ParoiVEFHyd.cpp) #
dimension 3

domaine DOM_BODY

Read_unsupported_ASCII_file_from_ICEM DOM_BODY trio_BODY_geo.asc

domaine dom

Extract_2D_from_3D DOM_BODY FRONT dom

ecrire_fichier dom dom2D.geom

dimension 2

Read_file dom dom2D.geom


# Longueurs 1.0 2.0 #

Pb_Hydraulique pb

# BEGIN PARTITION
Partition dom
{
        Partitionneur tranche { tranches 2 1 }
        Larg_joint 2
	Periodique 1 PERIO
        Nom_Zones DOM
}

END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #


Corriger_frontiere_periodique { Domaine dom Bord PERIO }


VEFPreP1B dis

Schema_Euler_explicite sch
Read sch 
{
        tinit 0.
	nb_pas_dt_max 3
        tmax 20.
        dt_min 1.e-7
        dt_max 1.e-1
	dt_start dt_calc
        dt_impr 0.001
        dt_sauv 20.
        seuil_statio 1.e-8  
        facsec 1.
}

Schema_Euler_implicite sch2
Read sch2
{
	tinit 0
	nb_pas_dt_max 3
	tmax 20.
	dt_min 1.e-7
	dt_max 1.e-1
	dt_impr 0.001
	dt_sauv 20.
	dt_start dt_calc
	seuil_statio 1.e-8
	facsec 1.
	facsec_max 200
	
	Solveur Implicite
	{
		seuil_convergence_solveur 1.e-10
		solveur gmres { diag seuil 1.e-10 impr controle_residu 1 nb_it_max 5 }
	}

}


Associate pb dom
Associate pb sch2

Discretize pb dis

Read pb
	{
   
   fluide_incompressible {
    	mu Champ_Uniforme 1 0.02
   	rho Champ_Uniforme 1 2
   }
   
   	Navier_Stokes_standard
		{
                solveur_pression GCP {
                        precond ssor { omega 1.5 }
                        seuil 1.e-8
                }
                convection { EF_stab { } }
                diffusion  {  }
                Sources  { Canal_perio { bord PERIO } }
		conditions_initiales
			{
                        vitesse Champ_fonc_xyz dom 2  1. 0. 
			}
		boundary_conditions
			{
			PERIO periodique  
			HAUT     paroi_fixe
			BAS      paroi_fixe
			}
		Traitement_particulier { Canal { dt_impr_moy_spat 6. }
		}
	}

	Postraitement
        	{
		format lml       
		Sondes
			{
			sonde_vinlet vitesse periode 0.001 Segment 90  0.5 0. 0.5 2.		
			}
		Champs dt_post 10.0
			{
			vitesse elem
			pression_Pa elem
			}
		}
	}

Solve pb
End
 
