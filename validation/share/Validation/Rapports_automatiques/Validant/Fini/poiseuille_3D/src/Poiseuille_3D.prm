Parameters {
	Title "Poiseuille 3D pipe flow with pressure loss"
	Description "Poiseuille flow in a cylindrical pipe.
	Description "Validated Model : Hydraulic with pressure drop"

	VersionTrio_U "1.5.4"

	ParametersTrio_U Type of problem : 3D Hydraulic
	ParametersTrio_U Type of fluide : Pure numerical properties
	ParametersTrio_U Discretization : VEFP1B
	ParametersTrio_U Time scheme : Euler_explicite
	ParametersTrio_U Solving of equations : Navier Stokes equation with differents convection schemas	
	ParametersTrio_U Convection QDM : Amont, Muscl, EF, Negligeable
	ParametersTrio_U Turbulence model : none
	ParametersTrio_U Type of boundary conditions : Inlet: Pressure imposed
	ParametersTrio_U Type of boundary conditions : Oulet: Pressure imposed

	TestCase EF_defaut_bar poise_tetra_EF1 
	TestCase negligeable_grossier poise_tetra_negligeable   
	TestCase muscl_grossier poise_tetra_muscl
	TestCase amont_raffine poise_tetra_raff
	TestCase amont_grossier poise_tetra /**/

	Author "R. PAILLE"
	IncludeData 2
}
Chapter {
	Title "Tests Description"
	Figure {
		Title "Geometry"
		Description "Dimensions : R=1 m, L=20 mm"
		Width 12cm
		picture ./Geometrie_Poiseuille_3D.png
	}
	visu {
	Title "Coarse mesh"
	mesh amont_grossier/poise_tetra.lata dom
	normal3D 0.5 0.3 0.8
	up3D -0.1 0.9 -0.3
	description "Mesh : 17 000 tetraedra mesh"
	description "Type of cells : tetraedral volumes generated by GMSH and "extrude" command of Trio_U"
	}
	visu {
	Title "Refined mesh"
	mesh amont_raffine/poise_tetra_raff.lata dom
	normal3D 0.5 0.3 0.8
	up3D -0.1 0.9 -0.3
	description "Mesh : 177 000 tetraedra mesh"
	description "Type of cells : tetraedral volumes generated by GMSH and "extrude" command of Trio_U"
	}
	Table {
		Title "Conditions"
		Description "Initial conditions"
		Description "......Hydraulic..............velocity in Z dimension=0.5m/s."
		Description ""
		Description "Boundary conditions"
		Description "......INLET  pressure imposed  P=1.6 b"
		Description "......OUTLET pressure imposed  P=0 b"
		Description "......WALL                     fixed wall"
		Description ""
	}
	Table {
		Title "Physical properties"
		Description "\latex_(\textbf{Incompressible case:}\latex_)"	
		Description "$\rho$ = 2 kg/m$^3$"
		Description "$\mu$ = 0.02 kg/ms"
	}
}
Chapter {
	Title "Results"
	Description "Representation of velocity profile"
	Figure {
		Title "Analytical result"
		Description "The analytical result is given by Vz=1-(X$^2$+Y$^2$)"
		Width 12cm
		Dimension 2
		LabelX "pipe diameter (m)"
		RangeX -1 1
		LabelY "Vz (m/s)"
		Include_Description_Curves 0
		Curve {
			legend "Analytical"
			Fonction 1-(x*x)
			Style lines
			TypeLigne 1
		}
		Curve {
			legend "Negligeable"
			file ./negligeable_grossier/moyenneSonde.plot
			columns ($1) ($2)
			Style lines
			TypeLigne 2
		}
		Curve {
			legend "EF"
			file ./EF_defaut_bar/moyenneSonde.plot
			columns ($1) ($2)
			Style lines
			TypeLigne 3
		}
		Curve {
			legend "Muscl"
			file ./muscl_grossier/moyenneSonde.plot
			columns ($1) ($2)
			Style lines
			TypeLigne 4
		}
		Curve {
			legend "Amont"
			file ./amont_grossier/moyenneSonde.plot
			columns ($1) ($2)
			Style lines
			TypeLigne 5
		}
		Curve {
			legend "Amont refined"
			file ./amont_raffine/moyenneSonde.plot
			columns ($1) ($2)
			Style lines
			TypeLigne 6
		}
	}
	Table {
		Title "Conclusion"
		Description "EF scheme is nearest to analytical solution."
		Description "the result is not improved by refined mesh with Amont convection scheme."
		Description "the over convection schemes are not tested with refined mesh."
		Description ""
	}
	Table {
		Title "Recommandations for users"
		Description "It's recommanded to use EF convection scheme, the other schemes show numerical diffusion."
	}
}
Chapter {
	Title "Computer performance"
	tableau_performance {
	}
}
