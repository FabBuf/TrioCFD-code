sed "s/Bas     frontiere_ouverte_vitesse_imposee champ_front_uniforme 2 0. 100000./Bas     frontiere_ouverte_vitesse_imposee champ_front_uniforme 2 0. 70710.7/" modifie.data > MixConv_VEF.data
rm modifie.data
mv MixConv_VEF.data MixConv.data
sed "s/VDF/VEF/" post_run > modifie
mv modifie post_run
