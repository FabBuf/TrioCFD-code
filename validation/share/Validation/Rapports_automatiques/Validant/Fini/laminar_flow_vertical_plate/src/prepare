sed "s/VDF/VEF/" post_run > modifie
mv modifie post_run
sed "s/tmax  10./tmax  0.00017 /" MixConv.data > modifie.data
mv modifie.data MixConv.data


cd ../MC_VEF_Bouss_amont_2
cp ../MixConv_VEF.data .
cp ../MC_post_run .
