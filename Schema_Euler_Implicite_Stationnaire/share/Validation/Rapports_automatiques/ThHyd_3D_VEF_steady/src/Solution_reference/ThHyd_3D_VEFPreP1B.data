
# Thermohydraulique 3D VEFPreP1B #
# PARALLEL OK 5 #
dimension 3    
Scheme_euler_implicit sch
Read sch
{
	tinit 0.
	tmax 500.
        dt_min 1.e-9
        dt_max 1.
        dt_impr 0.1
        dt_sauv 500.
	facsec 10
	facsec_max 50
        seuil_statio 1.e-8
	Solveur implicite {
	    solveur gmres { diag seuil 1.e-13 impr controle_residu 1 nb_it_max 3 } 
}
}

Pb_Thermohydraulique pb

Domaine dom_fluide
# BEGIN MESH #
Read_file dom_fluide cylindre1270_2.geom
# END MESH #

# BEGIN PARTITION
Partition dom_fluide
{
	Partition_tool metis { Nb_parts 2 }
	Larg_joint 2
	zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom_fluide
END SCATTER #

VEFPreP1B dis

Fluide_Incompressible fluide
Read fluide
{
	mu Champ_Uniforme 1 2e-09
	rho Champ_Uniforme 1 2
        lambda Champ_Uniforme 1 0.02
	Cp Champ_Uniforme 1 500
        beta_th Champ_Uniforme 1 0.003
}


Champ_Uniforme gravite
Read gravite 3 0. -9.81 0.
Associate fluide gravite

Associate pb dom_fluide
Associate pb fluide
Associate pb sch
Discretize pb dis

Read pb
{
        Navier_Stokes_standard
        {
     		solveur_pression GCP { precond ssor { omega 1.5 } seuil 1.e-15
		impr } 
                convection { amont }
                diffusion { }
                sources { boussinesq_temperature { T0 410. } }
                initial_conditions {
                        vitesse Champ_Uniforme 3 0. 0. 0.
                        }
                boundary_conditions {
                        HAUT   paroi_fixe
                        BAS    paroi_fixe
                        DROIT  paroi_fixe
                        GAUCH  paroi_fixe
                }
        }
        Convection_Diffusion_Temperature
        {
                diffusion {  }
                convection { amont }
                boundary_conditions 
                {	
			HAUT   Paroi_flux_impose 		Champ_front_Uniforme 1 0.
                        BAS    Paroi_flux_impose 		Champ_front_Uniforme 1 0.
                        DROIT  Paroi_temperature_imposee 	Champ_Front_Uniforme 1 323.15 
                        GAUCH  Paroi_flux_impose 		Champ_front_Uniforme 1 400.
                }
                initial_conditions { Temperature Champ_fonc_xyz dom_fluide 1 538.911933+((323.150000-538.911933)/0.02)*((SQRT(x^2+y^2))-0.01) }
        }
        Post_processing
        {	
                Probes
                { 			
                	T_A1 			temperature 		periode 1 	segment 10 0.011 0. 0.01  0.029 0. 0.01
                	T_A2 			temperature 		periode 1 	segment 10 0.011 -0.03 0.01  0.011 0.03 0.01
              		P_A1 			pression		periode 1 	segment 10 0.011 0. 0.01  0.029 0. 0.01
                	P_A2 			pression 		periode 1 	segment 10 0.011 -0.03 0.01  0.011 0.03 0.01
              		Vx_A1 			vitessex		periode 1 	segment 10 0.011 0. 0.01  0.029 0. 0.01
                	Vx_A2 			vitessex 		periode 1 	segment 10 0.011 -0.03 0.01  0.011 0.03 0.01
              		Vy_A1 			vitessey		periode 1 	segment 10 0.011 0. 0.01  0.029 0. 0.01
                	Vy_A2 			vitessey 		periode 1 	segment 10 0.011 -0.03 0.01  0.011 0.03 0.01

                }
		Format lata_v2
		Champs binaire dt_post 1e+4	
                {
			pression som
                        temperature som
			vitesse som
                }
        }
}

Solve pb
End
