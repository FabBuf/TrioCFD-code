Parameters {
	Title "Steady 2D Navier-Stokes VEF "
	Author "mp250145"
	VersionTrio_U 1.7.3 

	Description Test of the new Time scheme Implicit_Euler_steady_scheme with Solveur implicit_steady using a dual time step for the steady problems
        Description Comparaison with the exact solution

	ParametresTrio_U Type of problem: Steady Navier-Stokes
	ParametresTrio_U Discretizations: VEF P0 and VEF P0P1
	ParametresTrio_U Type of meshes:  Triangles
	ParametresTrio_U Pressure solver: GCP with SSOR preconditioneur
	ParametresTrio_U Time scheme: Implicit_Euler_steady_scheme with Solveur implicit_steady
	ParametresTrio_U Convection scheme: Amont, Muscl, EF_stab alpha = 0.2, EF_stab alpha = 0.7

	TestCase Amont Navier_Stokes_2d_steady.data /*jdd en annexe*/ 
	TestCase Muscl Navier_Stokes_2d_steady.data 
	TestCase EF_stab_alpha_0.2 Navier_Stokes_2d_steady.data 
	TestCase EF_stab_alpha_0.7 Navier_Stokes_2d_steady.data 

	




}




Chapter {
	Title "Convergence VEF P0" 

	figure { 
		title "Evolution of residue"
		include_description_curves 0
		labelX "TIME"
		labelY "residu=max|Ri|"
		logX
		logY
		curve {
			file ./Amont/Navier_Stokes_2d_steady.dt_ev
			columns ($1) ($4)
			legend "Amont scheme: residue"
			Style linespoints
			TypeLigne 1
			TypePoints 7
			PointSize 2
		}
		curve {
			file ./Muscl/Navier_Stokes_2d_steady.dt_ev
			columns ($1) ($4)
			legend "Muscl scheme: residue"
			Style linespoints
			TypeLigne 20
			TypePoints 7
			PointSize 2
		}
		curve {
			file ./EF_stab_alpha_0.2/Navier_Stokes_2d_steady.dt_ev
			columns ($1) ($4)
			legend "EF_stab_alpha_0.2 scheme: residue"
			Style linespoints
			TypeLigne 15
			TypePoints 7
			PointSize 2
		}
		curve {
			file ./EF_stab_alpha_0.7/Navier_Stokes_2d_steady.dt_ev
			columns ($1) ($4)
			legend "EF_stab_alpha_0.7 scheme: residue"
			Style linespoints
			TypeLigne 10
			TypePoints 7
			PointSize 2
		}
	}
}

