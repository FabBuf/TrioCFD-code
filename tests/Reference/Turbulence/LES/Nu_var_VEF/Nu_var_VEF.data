# Thermohydraulique 2D VEF Boussinesq avec Nu=Nu(T) #
# PARALLEL OK #
dimension 2
Pb_Thermohydraulique_Turbulent pb
Domaine dom
# BEGIN MESH #
Mailler dom
{
	Pave Cavite 
		{
		Origine 0. 0.
     		Nombre_de_Noeuds 10 10
		Longueurs 0.01 0.01
		}
		{
		Bord Gauche X = 0.  0. <= Y <= 0.01
		Bord Haut Y = 0.01  0. <= X <= 0.01
		Bord Bas Y = 0.     0. <= X <= 0.01
		Bord Droit X = 0.01 0. <= Y <= 0.01
		}
}

Trianguler_H dom

# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partitionneur metis { Nb_parts 2 }
	Larg_joint 2
	Nom_Zones DOM
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEFPreP1B dis

Schema_Euler_Explicite sch
Lire sch
{
	nb_pas_dt_max 10
	tinit 0.
	tmax 1.2
	dt_min 0.0001
	dt_max 0.1
	dt_impr 0.001
	dt_sauv 100
	seuil_statio 1.e-8
        diffusion_implicite 0
}

Associate pb dom
Associate pb sch
Discretize pb dis
Lire pb
{
Fluide_Incompressible
{
	mu champ_fonc_tabule { pb temperature } 1 
        {
		2
		0 	10
		1.0e-5	1.1e-5     
        }
	rho Champ_Uniforme 1 1.17
	lambda Champ_Uniforme 1 0.0262 
	Cp Champ_Uniforme 1 1006
	beta_th Champ_Uniforme 1 3.41e-3
	gravite Champ_Uniforme 2 0. -9.81
}
	Navier_Stokes_turbulent
	{
		solveur_pression GCP { precond ssor { omega 1.5 } seuil 1.e-12 impr }
		convection { muscl }
		diffusion { }
		modele_turbulence Sous_maille
			{
			Turbulence_paroi loi_standard_hydr
			} 
		sources { boussinesq_temperature { T0 5. } }
		conditions_initiales 
                        {
			vitesse Champ_Uniforme 2 0. 0.
			}
		boundary_conditions {
			Haut paroi_fixe
			Bas paroi_fixe
			Gauche paroi_fixe
			Droit paroi_fixe
		}
	}
	Convection_Diffusion_Temperature_Turbulent
	{
		diffusion { }
		convection { muscl }
                Modele_turbulence Prandtl { Turbulence_paroi LOI_STANDARD_HYDR_SCALAIRE }
		boundary_conditions 
		{
			Haut paroi_adiabatique
			Bas paroi_adiabatique
 			Gauche Paroi_temperature_imposee Champ_Front_Uniforme 1 10.
			Droit Paroi_temperature_imposee Champ_Front_Uniforme 1 0
		}
		conditions_initiales { Temperature Champ_Uniforme 1 5. }
	}

	Postraitement
	{
		Sondes
		{
			sonde_pression pression periode 0.01 points 1 0.0045 0.0045
			sonde_vitesse vitesse periode 0.01 segment 10 0.0005 0.005 0.0095 0.005
			sonde_vit vitesse periode 0.01 segment 11 0.0005 0. 0.0005 0.01
			sonde_temp temperature periode 0.01 segment 21 0. 0.0055 0.01 0.0055
		}
		Champs dt_post 0.1
		{
			vitesse som
			vitesse elem
			temperature
			viscosite_cinematique elem
		}
	}
}

Solve pb
Fin
