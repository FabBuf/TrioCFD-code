# SIMULATION D UN CANAL PLAN 3D EN LES #
# EN THERMOHYDRAULIQUE : Hypothese de Boussinesq et schema quick #
# PARALLEL OK 8 #
dimension 3
Domaine dom

# BEGIN MESH #
Mailler dom
	{
	Pave Cavite
		{
		Origine 0. 0. 0.
		Nombre_de_Noeuds 8 14 10
		Longueurs  12.56637061 2. 3.141592653
		tanh 0.90180
		}
		{
		Bord periox   X = 0.            0. <= Y <= 2.0           0. <= Z <= 3.141592653
		Bord periox   X = 12.56637061   0. <= Y <= 2.0           0. <= Z <= 3.141592653
		Bord perioz   Z = 0.            0. <= X <= 12.56637061   0. <= Y <= 2.0
		Bord perioz   Z = 3.141592653   0. <= X <= 12.56637061   0. <= Y <= 2.0
		Bord bas      Y = 0.            0. <= X <= 12.56637061   0. <= Z <= 3.141592653
		Bord haut     Y = 2.            0. <= X <= 12.56637061   0. <= Z <= 3.141592653
		}
	}
# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partitionneur tranche { tranches 6 1 1 }
	Larg_joint 1
	Nom_Zones DOM
	Periodique 2 periox perioz
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

Pb_Thermohydraulique_Turbulent pb
VDF dis

Runge_Kutta_ordre_3 sch
Lire sch
{
    nb_pas_dt_max 10
    tinit 0.
    tmax 10000.
    dt_start dt_min
    dt_min 1.e-7
    dt_max 1.e-1
    dt_impr 1e-5
    dt_sauv 500.
    seuil_statio 1.e-8
}

Fluide_Incompressible fluide
Lire fluide
{
    mu Champ_Uniforme		1 2.84e-5
    rho Champ_Uniforme		1 0.0976
    lambda Champ_Uniforme	1 22.1e-2
    Cp Champ_Uniforme		1 5193.
    beta_th Champ_Uniforme	1 1.9954e-3
}

Champ_Uniforme gravite
Lire gravite 3 0 0 0

Associate fluide gravite
Associate pb dom
Associate pb sch
Associate pb fluide

Discretize pb dis

# Ampli_bruit !=0 peut creer des ecarts entre sequentiel et parallele #
# Donc on le met a 0, sinon prendre Ampli_bruit 0.1 #
Lire pb
	{
	Navier_Stokes_Turbulent
		{
		solveur_pression GCP
			{
			precond ssor { omega 1.5 } 
			seuil 1e-7
			impr
			}
		convection { centre }
		diffusion { }
		Sources
			{
			Canal_perio { bord periox } ,
			Boussinesq_temperature { T0 500 }
			}
		conditions_initiales
			{
			vitesse champ_init_canal_sinal 3  { Ucent 1.2062 h 1.0  ampli_sin 0.3 omega 2. ampli_bruit 0. }
			}
		boundary_conditions
			{
			periox   periodique 
			haut     paroi_fixe
			bas      paroi_fixe
			perioz   periodique 
			}
		Modele_turbulence sous_maille_smago_dyn
			{
			stabilise moy_euler	
			turbulence_paroi negligeable
			}
		}


	Convection_Diffusion_Temperature_Turbulent
		{
		diffusion { }
		convection { amont }
		Modele_turbulence sous_maille_dyn 
		{ 
			stabilise moy_euler
			turbulence_paroi negligeable_scalaire 
		}
		boundary_conditions 
			{
			periox periodique 
			perioz periodique 
 			haut   paroi_temperature_imposee Champ_Front_Uniforme 1 505
			bas    paroi_temperature_imposee Champ_Front_Uniforme 1 500
			}
		conditions_initiales { Temperature Champ_Fonc_xyz dom 1 500.+y*(505.-500.)/2. }
		}

	Postraitement
        	{       
                Sondes
                	{ 
                        sonde_vit  vitesse periode 10. segment 65	4. 0. 1.
									4. 2. 1.
                        sonde_temp temperature periode 10. segment 65	4. 0. 1.
									4. 2. 1. 
			}
		Champs dt_post 20.
			{
			pression elem
			vitesse elem
			temperature elem
			viscosite_turbulente elem
			}
		}
	}

Solve pb
Fin
Traitement_particulier
			{
			Canal
				{
				u_inst 20.
				nb_points 100
				stats 300. 10000. 10.
				profil_nu_t
				stats_thermo
				}
			}
Modele_turbulence Prandtl_sous_maille_dyn 
